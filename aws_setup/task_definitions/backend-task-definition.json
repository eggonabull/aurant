{
  "family": "aurant-dev-backend",
  "taskRoleArn": "arn:aws:iam::082690973139:role/aurant-dev-ecs-task-role",
  "executionRoleArn": "arn:aws:iam::082690973139:role/aurant-dev-ecs-task-execution-role",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "082690973139.dkr.ecr.us-west-2.amazonaws.com/aurant-dev-backend:aa013b65d85157f33971a90d17a3d87d3d2e739f",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "RUST_LOG",
          "value": "info"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/aurant-dev-backend",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  }
}
